page {
  font-family: Microsoft YaHei, MicrosoftYaHei-Normal, serif;
}

#main_bg {
  height: 490rpx;
  background-color: #6C7BFF;
}
#main_bg image {
  width: 100%;
}

.module {
  width: 100%;
  height: 215rpx;
  border-radius: 24rpx;
  font-size: 44rpx;
  color: white;
  display: flex;
  align-items: center;
}

.elastic {
  transition-timing-function: cubic-bezier(0.19, 6.98, 0.08, -2.53);
  transition-duration: 0.3s;
}
.elastic:active {
  transition-timing-function: linear;
  transition-duration: 0.1s;
  transform: scale(0.93);
}

#launch {
  width: 100%;
  font-size: 28rpx;
  font-weight: normal;
  line-height: 56rpx;
  color: #e1d4f2;
  text-align: center;
}

.icon_o_module {
  border-radius: 50%;
  width: 108rpx;
  height: 108rpx;
  line-height: 108rpx;
  vertical-align: middle;
  text-align: center;
  font-size: 64rpx;
  font-weight: bold;
}

#after_login {
  background-color: rgba(188, 215, 66, 0.7490196078);
}
#after_login #info {
  font-size: 36rpx;
  margin-left: 39rpx;
}
#after_login #portrait {
  margin-left: 62rpx;
}
#after_login #portrait image {
  border: 6rpx solid white;
  border-radius: 50%;
  width: 116rpx;
  height: 116rpx;
}

#login_dec {
  height: 200rpx;
  position: absolute;
  right: 0;
}

#countainer {
  position: absolute;
  top: 30rpx;
  width: 100%;
}
